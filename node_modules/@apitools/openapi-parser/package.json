{
  "name": "@apitools/openapi-parser",
  "author": "Mrinmoy Majumdar",
  "license": "MIT",
  "version": "0.0.30",
  "description": "Resolve OpenAPI Refs and/or convert Swagger 2.0 to OpenAPI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrin9/apitools.git",
    "directory": "packages/openapi-parser"
  },
  "module": "dist/openapi-parser.umd.js",
  "main": "dist/openapi-parser.umd.js",
  "exports": {
    ".": {
      "import": "./dist/openapi-parser.mjs",
      "require": "./dist/openapi-parser.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "test-debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "swagger-client": "^3.18.5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-eslint": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "jest": "^28.1.3",
    "vite": "^3.0.7",
    "typescript": "^4.6.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "openapi",
    "parser",
    "swagger2openapi",
    "swagger-converter",
    "dereference",
    "json",
    "yaml",
    "swagger-parser"
  ]
}
